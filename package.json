  "lint-staged": {
    "src/**/*.{js,jsx,json,css,scss}": [
      "prettier --single-quote --write",
      "git add"
    ],
    "common/**/*.{js,json}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main*.js",
    "start": "ln -sf .env.devapp.local .env && NODE_PATH=src react-scripts start",
    "build": "ln -sf .env.prodapp.local .env && NODE_PATH=src react-scripts build",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "beauty": "prettier --single-quote --write 'src/**/*.{js,jsx,css,scss}'",
    "build-css": "node-sass-chokidar src/ -o src/ --recursive",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "tar": "tar -cjvf ysj-client.tar.bz2 build  --exclude=build/*.map  --exclude=build/*.map.* "
  },
  "proxy": "http://localhost:3010"
